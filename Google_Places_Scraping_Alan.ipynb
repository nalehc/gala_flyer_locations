    "def CD_Conversion(substring, CD_dict):\n",
    "    str_list = substring.split(' ')\n",
    "    Neighborhood = CD_dict[str_list[0]]\n",
    "    Number = str_list[2]\n",
    "    if len(Number) < 2:\n",
    "        Number = \"0\" + Number\n",
    "    return int(Neighborhood + Number)"
   ]
